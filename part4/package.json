{
  "name": "bloglist",
  "version": "1.0.0",
  "description": "bloglist",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node index.js",
    "dev": "cross-env NODE_ENV=development nodemon index.js",
    "start2": "node index.js",
    "dev2": "nodemon --inspect index.js",
    "build:ui": "rm -rf build && cd ../Fin_test_react && npm run build --prod && cp -r build ../Fin_test_node/",
    "deploy": "git push heroku master",
    "deploy:full": "npm run build:ui && git add . && git commit -m uibuild && npm run deploy",
    "logs:prod": "heroku logs --tail",
    "lint": "eslint",
    "lint:all": "eslint .",
    "test": "cross-env NODE_ENV=test jest --verbose --runInBand",
    "test2": "clear && jest --verbose",
    "test:sp": "jest -u -t=\"favorite blog\"",
    "test:file": "clear && npm test -- tests/listAPI.test.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "mongoose": "^5.10.11",
    "mongoose-unique-validator": "^2.0.3",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^7.14.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "supertest": "^6.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
